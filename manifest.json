{
  "name": "meet-reaction-keydown",
  "description": "Enable keydown to run the reaction function of google meet.",
  "manifest_version": 3,
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "css": ["style.css"],
      "js": ["index.js"]
    }
  ],
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
